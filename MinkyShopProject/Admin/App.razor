<Router AppAssembly="@typeof(App).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(Admin.Shared.MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
        <PageTitle>Not found</PageTitle>
        <LayoutView Layout="@typeof(Admin.Shared.EmptyLayout)">
            <MinkyShopProject.Admin.Shared.NotFoundComponent />
        </LayoutView>
    </NotFound>
</Router>